{
  "name": "events-be",
  "version": "0.0.0",
  "private": true,
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "start": "tsc & node .",
    "dev": "nodemon server.ts",
    "test": "jest",
    "test:unit": "jest --selectProjects UnitTests",
    "test:integration": "jest --selectProjects IntegrationTests",
    "reset-prisma": "npx prisma migrate dev reset --force",
    "reset-migs": "prisma migrate dev --name init",
    "insert-data": "psql -f ./data.sql postgresql://postgres:postgres@localhost:5432/postgres",
    "reset-db": "npm run reset-migs && npm run insert-data"
  },
  "dependencies": {
    "@babel/plugin-transform-typescript": "^7.20.2",
