{
  "name": "events-be",
  "version": "0.0.0",
  "private": true,
  "main": "dist/index.js",
  "jest": {
    "testMatch": [
      "<rootDir>/**/*.test.ts"
    ]
  },
  "babel": {
    "env": {
      "test": {
        "presets": [
          "@babel/preset-env",
          "@babel/preset-typescript"
        ],
        "targets": {
          "node": "current"
        },
        "plugins": [
          "@babel/plugin-transform-modules-commonjs",
          "@babel/plugin-transform-typescript"
        ]
      }
    }
  },
  "scripts": {
    "build": "tsc",
    "start": "tsc & node .",
    "dev": "nodemon server.ts",
    "test": "jest",
    "reset-prisma": "npx prisma migrate dev reset --force",
    "reset-migs": "prisma migrate dev --name init",
    "insert-data-prisma": "node ./test/insert_test_data.js",
    "insert-data": "psql -f ./data.sql postgresql://postgres:postgres@localhost:5432/postgres",
    "reset-db": "npm run mig-delete && npm run reset-migs && npm run insert-data"
  },
  "dependencies": {
    "@babel/plugin-transform-typescript": "^7.20.2",
    "@babel/preset-env": "^7.20.2",
    "@babel/preset-typescript": "^7.18.6",
    "@prisma/client": "^4.6.0",
    "@types/express": "^4.17.14",
    "@types/node": "^18.11.9",
    "cookie-parser": "~1.4.4",
    "cors": "^2.8.5",
    "debug": "~2.6.9",
    "express": "~4.16.1",
    "http-errors": "~1.6.3",
    "jade": "~1.11.0",
    "jest-mock-extended": "^3.0.1",
    "morgan": "~1.9.1",
    "nodemon": "^2.0.20",
    "typescript": "^4.8.4",
    "web3": "^1.8.1",
    "webpack": "^5.75.0"
  },
  "devDependencies": {
    "@babel/plugin-transform-modules-commonjs": "^7.19.6",
    "@types/cors": "^2.8.12",
    "@types/jest": "^29.2.3",
    "babel-cli": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "jest": "^29.3.1",
    "prisma": "^4.6.0",
    "superagent": "^8.0.3",
    "supertest": "^6.3.1"
  }
}