{
  "name": "events-be",
  "version": "0.0.0",
  "private": true,
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "start": "tsc & node .",
    "dev": "nodemon index.ts",
    "reset-prisma": "npx prisma migrate dev reset --force",
    "reset-migs": "prisma migrate dev --name init",
    "insert-data-prisma": "node ./test/insert_test_data.js",
    "mig-delete": "rmdir /s /q prisma\\migrations",
    "insert-data": "sh ./test/insert_test_data.sh",
    "insert-data2": "psql -f ./data.sql postgresql://postgres:postgres@localhost:5432/postgres",
    "reset-db": "npm run mig-delete && npm run reset-migs && npm run insert-data2"
  },
  "dependencies": {
    "@prisma/client": "^4.6.0",
    "@types/express": "^4.17.14",
    "@types/node": "^18.11.9",
    "cookie-parser": "~1.4.4",
    "cors": "^2.8.5",
    "debug": "~2.6.9",
    "express": "~4.16.1",
    "http-errors": "~1.6.3",
    "jade": "~1.11.0",
    "morgan": "~1.9.1",
    "nodemon": "^2.0.20",
    "typescript": "^4.8.4",
    "webpack": "^5.75.0"
  },
  "devDependencies": {
    "@types/cors": "^2.8.12",
    "prisma": "^4.6.0"
  }
}